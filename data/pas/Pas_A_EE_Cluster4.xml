		 </preferenceUtilities>
		 <agentclassname>org.volante.abm.agent.bt.LaraBehaviouralComponent</agentclassname>
	</bofactory>
		
	<bofactory class="org.volante.abm.comi.decision.pa.ComiSwitchBackPa$ComiSwitchBackPaFactory">
