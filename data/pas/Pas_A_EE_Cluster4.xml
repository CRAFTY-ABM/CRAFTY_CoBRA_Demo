		 </preferenceUtilities>
		 <agentclassname>org.volante.abm.agent.bt.LaraBehaviouralComponent</agentclassname>
	</bofactory>
	
	<bofactory class="org.volante.abm.decision.pa.DoNothingPa$DoNothingPaFactory">
